<h4>Client information</h4>
<div class="row">


<table class="table">
  <tbody>
    <tr>
      <td>
        Contact Name:
      </td>
      <td>
        <%= current_user.contact_name %>
      </td>
    </tr>
    <tr>
      <td>
        Company:
      </td>
      <td>
        <%= current_user.company %>
      </td>
    </tr>
    <tr>
      <td>
        Email:
      </td>
      <td>
        <%= current_user.email %>
      </td>
    </tr>
    <tr>
      <td>
        Phone:
      </td>
      <td>
        <%= current_user.phone %>
      </td>
    </tr>
    <tr>
      <td>
        Address:
      </td>
      <td>
        <%= current_user.company_address %> <%=current_user.city%> <%= current_user.state.name%>, <%= current_user.zipcode%>
      </td>
    </tr>
  </tbody>
</table>
</div>
<h3>Results</h3>


<div class="row">
	<% current_user.results.each do |result| %>
		<%= link_to "Download Result", result.attachment.url%>
		<p><%= result.created_at.strftime("%m/%d/%Y")%> </p>
	<% end %>
</div>
<%= link_to "Log Out", session_path, method: :delete %>
